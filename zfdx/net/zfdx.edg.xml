<edges>
    <!-- 入边：从外部节点到中心交叉口 -->
    <edge id="n_t" from="n" to="t" numLanes="5"/>
    <edge id="w_t" from="w" to="t" numLanes="4"/>
    <edge id="s_t" from="s" to="t" numLanes="5"/>
    <edge id="e_t" from="e" to="t" numLanes="5"/>
    
    <!-- 出边：从中心交叉口到外部出口节点 -->
    <edge id="t_n" from="t" to="n_out" numLanes="5"/>
    <edge id="t_w" from="t" to="w_out" numLanes="5"/>
    <edge id="t_s" from="t" to="s_out" numLanes="5"/>
    <edge id="t_e" from="t" to="e_out" numLanes="5"/>
</edges>